package bean;

public class OrderBean {

	private String name;
	private float price;
	private int amount;
	private String userID;

	public OrderBean(){
	}
	
	public String getName(){
		return name;
	}

	public float getPrice(){
		return price;
	}
	
	public int getAmount(){
		return amount;
	}
	
	public String getuserID(){
		return userID;
	}
	
	public void setAmount(int amount){
		this.amount = amount;
	}


}